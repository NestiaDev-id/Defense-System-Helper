{
  "version": 2,
  "builds": [
    {
      "src": "api/index.ts", 
      "use": "@vercel/node"
    },
    {
      "src": "python/main.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [

    {
      "src": "/py/(.*)", 
      "dest": "python/main.py" 
    },
    {
      "src": "/(.*)", 
      "dest": "api/index.ts"
    }
  ]
}